# Push and pop directories on directory stack
alias pu='pushd'
alias po='popd'

# Basic directory operations
alias ..='cd ..'
alias ...='cd ../..'
alias -- -='cd -'

# List directory contents
alias lsa='ls -lah --color'
alias l='ls -lahG --color'
alias ll='ls -l --color'
alias lsd='ls -alrth --color'
